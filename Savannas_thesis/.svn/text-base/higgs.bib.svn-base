@Misc{epaps, note = "See EPAPS Document No.~xxxx for descriptions of MVA input variables." }

@Misc{defsjetlepdetamax,  note = "Maximum $\Delta\eta$ between the charged lepton and the leading or second leading jet." }
@Misc{defstopovissumpt,  note = "Scalar sum of the $p_T$ of the visible particles, including the charged lepton and jets." }
@Misc{defslepqeta,  note = "Product of the lepton charge and its pseudorapidity." }
@Misc{defstopoaplanarity,  note = "Aplanarity is defined as $3\lambda_{3}/2$, where $\lambda_{3}$ is the smallest eigenvalue of the normalized momentum tensor 
      $S^{\alpha \beta}=(\sum_{i}p_{i}^{\alpha}p_{i}^{\beta})/(\sum_{i} |\vec{p_{i}}|^{2})$ , where $\alpha,\beta=1,2,3$ correspond to the 
      $x,y,z$ momentum components, and $i$ runs over all visible objects." }
@Misc{defsnupz,  note = "The $p_Z$ of the neutrino candidate is estimated by constraining the charged lepton and the neutrino system to the mass of the $W$ boson and choosing the lowest magnitude solution." }
@Misc{defslnujmB,  note = "Invariant mass of the system consisting of the charged lepton, reconstructed neutrino, and second leading jet." }
@Misc{defslnulepdeta,  note = "$\Delta\eta$ between the $\ell\nu$ system and the charged lepton." }
@Misc{defslnucmcostheta,  note = "Cosine of the angle between the charged lepton and the proton beam axis in the center of mass of the $\ell\nu$ system." }
@Misc{defslepsigma,  note = "${\cal{SIG}}_{\text{jets}}$ for $\ell$ is defined with respect to all jets in an event as $\sum_{\text{jets}}[p_T^{\text{jet}}\times\Delta\mathcal{R} (\ell,{\text{jet}})]/\sum_{\text{jets}}(p_T^{\text{jet}})$." }
